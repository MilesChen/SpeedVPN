            android:label="@string/profile" />
        <activity
            android:name="com.github.kr328.clash.ProxyActivity"
            android:configChanges="uiMode"
            android:exported="false"
            android:label="@string/proxy" />
        <activity
            android:name="com.github.kr328.clash.ProvidersActivity"
            android:configChanges="uiMode"
            android:exported="false"
            android:label="@string/providers" />
        <activity
            android:name="com.github.kr328.clash.LogsActivity"
            android:configChanges="uiMode"
            android:exported="false"
            android:label="@string/logs" />
        <activity
            android:name="com.github.kr328.clash.LogcatActivity"
            android:configChanges="uiMode"
            android:exported="false"
            android:label="@string/logcat" />
        <activity
            android:name="com.github.kr328.clash.SettingsActivity"
