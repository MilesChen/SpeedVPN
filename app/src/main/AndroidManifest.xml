            android:configChanges="uiMode"
            android:exported="false"
            android:label="@string/settings" />
        <activity
            android:name="com.github.kr328.clash.NetworkSettingsActivity"
            android:screenOrientation="portrait"
            android:configChanges="uiMode"
            android:exported="false"
            android:label="@string/network" />
        <activity
            android:name="com.github.kr328.clash.AppSettingsActivity"
            android:screenOrientation="portrait"
            android:configChanges="uiMode"
            android:exported="false"
            android:label="@string/app" />
        <activity
            android:name="com.github.kr328.clash.OverrideSettingsActivity"
            android:configChanges="uiMode"
